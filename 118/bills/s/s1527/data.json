  "introduced_at": "2023-05-10",
  "number": "1527",
  "official_title": "A bill to amend title 10, United States Code, to ensure that members of the Armed Forces and their families have access to the contraception they need in order to promote the health and readiness of all members of the Armed Forces, and for other purposes.",
  "popular_title": null,
  "related_bills": [],
  "short_title": "Access to Contraception for Servicemembers and Dependents Act of 2023",
  "sponsor": {
    "bioguide_id": "S001181",
    "district": null,
    "name": "Shaheen, Jeanne",
    "state": "NH",
    "title": "Sen.",
    "type": "person"
  },
  "status": "REFERRED",
  "status_at": "2023-05-10",
  "subjects": [
