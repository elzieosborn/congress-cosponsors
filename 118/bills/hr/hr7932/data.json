  "introduced_at": "2024-04-10",
  "number": "7932",
  "official_title": "To prohibit Federal funds from being made available to international financial institutions for the purposes of financing foreign shrimp farms, and for other purposes.",
  "popular_title": null,
  "related_bills": [],
  "short_title": "Save Our Shrimpers Act",
  "sponsor": {
    "bioguide_id": "N000026",
