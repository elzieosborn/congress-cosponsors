  "short_title": "Bring Animals Relief and Kibble Act of 2023",
  "sponsor": {
    "bioguide_id": "R000606",
    "district": "8",
    "name": "Raskin, Jamie",
    "state": "MD",
    "title": "Rep.",
    "type": "person"
