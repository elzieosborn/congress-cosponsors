    <summaries>
      <summary>
        <versionCode>00</versionCode>
        <actionDate>2017-03-20</actionDate>
        <actionDesc>Introduced in House</actionDesc>
        <updateDate>2017-05-04T19:25:25Z</updateDate>
        <text><![CDATA[ <p><b>Protecting Foods and Beverages from Government Attack Act of 2017</b></p> <p>This bill prohibits federal funding of print, radio, television or any other media advertisement, campaign, or form of publicity against the use of a food or beverage that is lawfully marketed under the Federal Food, Drug, and Cosmetic Act.</p>]]></text>
      </summary>
    </summaries>
    <title>Protecting Foods and Beverages from Government Attack Act of 2017</title>
    <titles>
      <item>
        <titleType>Display Title</titleType>
        <title>Protecting Foods and Beverages from Government Attack Act of 2017</title>
      </item>
      <item>
        <titleType>Official Title as Introduced</titleType>
        <title>To prohibit the use of Federal money for print, radio, television or any other media advertisement, campaign, or form of publicity against the use of a food or beverage that is lawfully marketed under the Federal Food, Drug, and Cosmetic Act.</title>
        <billTextVersionName>Introduced in House</billTextVersionName>
        <billTextVersionCode>IH</billTextVersionCode>
      </item>
