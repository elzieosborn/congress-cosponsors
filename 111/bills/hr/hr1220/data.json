  "introduced_at": "2009-02-26",
  "number": "1220",
  "official_title": "To amend title 49, United States Code, to provide certain exemptions to drivers of intrastate commercial motor vehicles engaged in agricultural purposes, and for other purposes.",
  "popular_title": null,
  "related_bills": [],
  "short_title": null,
