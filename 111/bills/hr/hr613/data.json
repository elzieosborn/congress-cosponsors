  "introduced_at": "2009-01-21",
  "number": "613",
  "official_title": "To amend title 10, United States Code, to provide for forgiveness of certain overpayments of retired pay paid to deceased retired members of the Armed Forces following their death.",
  "popular_title": null,
  "related_bills": [],
  "short_title": "Military Retiree Survivor Comfort Act",
  "sponsor": {
    "bioguide_id": "J000255",
    "district": "3",
    "name": "Jones, Walter B., Jr.",
    "state": "NC",
