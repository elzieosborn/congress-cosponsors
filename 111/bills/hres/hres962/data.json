    },
    {
      "acted_at": "2009-12-09T22:18:52-05:00",
      "action_code": "H12700",
      "bill_ids": [
        "hr4173-111"
      ],
      "references": [],
      "text": "The resolution waives clause 6(a) of rule XIII (requiring a two-thirds vote to consider a rule on the same day it is reported from the Rules Committee) against certain resolutions reported from the Rules Committee. The rule applies the waiver to any resolution reported on the legislative day of December 10, 2009, providing for further consideration or disposition of H.R. 4173.",
      "type": "action"
    },
