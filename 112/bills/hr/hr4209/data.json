  "introduced_at": "2012-03-19",
  "number": "4209",
  "official_title": "To amend title XXVII of the Public Health Service Act to limit co-payment, coinsurance, or other cost-sharing requirements applicable to prescription drugs in a specialty drug tier to the dollar amount (or its equivalent) of such requirements applicable to prescription drugs in a non-preferred brand drug tier, and for other purposes.",
  "popular_title": null,
  "related_bills": [],
  "short_title": "Patients' Access to Treatments Act of 2012",
  "sponsor": {
    "bioguide_id": "M001180",
    "district": "1",
    "name": "McKinley, David B.",
    "state": "WV",
    "title": "Rep.",
    "type": "person"
  },
  "status": "REFERRED",
  "status_at": "2012-03-19",
  "subjects": [
