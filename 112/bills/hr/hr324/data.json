  "introduced_at": "2011-01-19",
  "number": "324",
  "official_title": "To amend title 10, United States Code, to provide police officers, criminal investigators, and game law enforcement officers of the Department of Defense with authority to execute warrants, make arrests, and carry firearms.",
  "popular_title": null,
  "related_bills": [],
  "short_title": null,
  "sponsor": {
    "bioguide_id": "F000116",
