  "introduced_at": "2012-05-10",
  "number": "5731",
  "official_title": "To prohibit Federal assistance for telemedicine abortions and to ban interstate abortions using telemedicine technology.",
  "popular_title": null,
  "related_bills": [],
  "short_title": "Telemedicine Safety Act",
  "sponsor": {
    "bioguide_id": "K000362",
