  "short_title": "National Origin-Based Antidiscrimination for Nonimmigrants Act",
  "sponsor": {
    "bioguide_id": "C001080",
    "district": "28",
    "name": "Chu, Judy",
    "state": "CA",
    "title": "Rep.",
    "type": "person"
