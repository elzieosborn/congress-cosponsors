  "short_title": "Sanctioning Russia Act of 2025",
  "sponsor": {
    "bioguide_id": "F000466",
    "district": "1",
    "name": "Fitzpatrick, Brian K.",
    "state": "PA",
    "title": "Rep.",
    "type": "person"
  },
  "status": "REFERRED",
  "status_at": "2025-04-01",
  "subjects": [
    "International affairs"
