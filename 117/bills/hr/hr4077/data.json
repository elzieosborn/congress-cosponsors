  "short_title": "Closing the Meal Gap Act of 2021",
  "sponsor": {
    "bioguide_id": "A000370",
    "district": "12",
    "name": "Adams, Alma S.",
    "state": "NC",
    "title": "Rep.",
    "type": "person"
