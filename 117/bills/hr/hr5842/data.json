  "number": "5842",
  "official_title": "To deter, prevent, reduce, and respond to harassment in the workplace, including sexual harassment, sexual assault, and harassment based on protected categories; and to amend the Internal Revenue Code of 1986 to modify the tax treatment of amounts related to employment discrimination and harassment in the workplace, including sexual harassment, sexual assault, and harassment based on protected categories.",
  "popular_title": null,
  "related_bills": [],
  "short_title": "EMPOWER Act",
  "sponsor": {
    "bioguide_id": "F000462",
    "district": "21",
    "name": "Frankel, Lois",
    "state": "FL",
