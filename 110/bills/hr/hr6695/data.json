  "introduced_at": "2008-07-31",
  "number": "6695",
  "official_title": "To amend title 38, United States Code, to conform the mileage reimbursement rates used under the beneficiary travel program administered by the Secretary of Veterans Affairs to the mileage reimbursement rates for Government employees on official business who use privately owned vehicles, to eliminate all deductibles under the beneficiary travel program, to ensure that all veterans can participate in the beneficiary travel program, and for other purposes.",
  "popular_title": null,
  "related_bills": [],
  "short_title": null,
  "sponsor": {
    "bioguide_id": "W000799",
    "district": "1",
    "name": "Walz, Timothy J.",
    "state": "MN",
    "title": "Rep.",
    "type": "person"
  },
  "status": "REFERRED",
  "status_at": "2008-07-31",
  "subjects": [
    "Armed forces and national security",
