    <summaries>
      <summary>
        <versionCode>00</versionCode>
        <actionDate>2008-03-12</actionDate>
        <actionDesc>Introduced in House</actionDesc>
        <updateDate>2008-06-03T20:11:57Z</updateDate>
        <text><![CDATA[ <p>Amends the Illegal Immigration Reform and Immigrant Responsibility Act of 1996 to revise the termination date for the basic pilot employment eligibility confirmation program from 11 years after its beginning date to not later than December 31, 2018.</p>]]></text>
      </summary>
    </summaries>
