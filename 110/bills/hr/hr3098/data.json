  "introduced_at": "2007-07-19",
  "number": "3098",
  "official_title": "To amend title 49, United States Code, to provide certain exemptions to drivers of intrastate commercial motor vehicles engaged in agricultural purposes.",
  "popular_title": null,
  "related_bills": [],
  "short_title": null,
