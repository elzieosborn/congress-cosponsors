      <item>
        <titleType>Official Title as Introduced</titleType>
        <title>To reduce health care costs and promote improved health by providing supplemental grants for additional preventive health services for women.</title>
        <billTextVersionName>Introduced in House</billTextVersionName>
        <billTextVersionCode>IH</billTextVersionCode>
